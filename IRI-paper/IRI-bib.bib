@article{HawthorneStanley2008,
	Abstract = {Judging by our folk appraisals, then, knowledge and action are intimately related. The theories of rational action with which we are familiar leave this unexplained. Moreover, discussions of knowledge are frequently silent about this connection. This is a shame, since if there is such a connection it would seem to constitute one of the most fundamental roles for knowledge. Our purpose in this paper is to rectify this lacuna, by exploring ways in which knowing something is related to rationally acting upon it, defending one particular proposal against anticipated objections.},
	Author = {John Hawthorne and Jason Stanley},
